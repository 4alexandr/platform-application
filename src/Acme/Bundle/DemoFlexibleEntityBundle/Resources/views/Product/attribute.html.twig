{% extends 'AcmeDemoFlexibleEntityBundle::layout.html.twig' %}

{% block content %}

    <table class="mytable">
        <tr>
            <td>Id</td>
            <td>Code</td>
            <td>Backend storage</td>
            <td>Backend type</td>
            <td>Required</td>
            <td>Unique</td>
            <td>Translatable</td>
            <td>Created</td>
        </tr>
    {% for attribute in attributes %}
        <tr>
            <td>{{ attribute.id }}</td>
            <td>{{ attribute.code }}</td>
            <td>{{ attribute.backendStorage }}</td>
            <td>{{ attribute.backendType }}</td>
            <td>{{ attribute.required }}</td>
            <td>{{ attribute.unique }}</td>
            <td>{{ attribute.translatable }}</td>
            <td>{{ attribute.created|date('Y-m-d h:i:s') }}</td>
        </tr>
    {% endfor %}
    </table>

{% endblock %}